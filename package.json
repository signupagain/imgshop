{
	"name": "imgshop",
	"version": "0.0.0",
	"scripts": {
		"dev": "vite --open",
		"build": "vue-tsc --noEmit && vite build",
		"preview": "vite preview",
		"lint": "eslint . --fix --ignore-path .gitignore",
		"toc": "i18next-resources-for-ts toc -i src/locale/zhHantTW -o src/locale/output/zhHantTW.ts",
		"interface": "i18next-resources-for-ts interface -i src/locale/zhHantTW -o src/@types/resources.d.ts",
		"p": "prettier --write ."
	},
	"dependencies": {
		"core-js": "^3.37.1",
		"i18next": "^23.16.0",
		"i18next-vue": "^5.0.0",
		"nanoid": "^5.0.7",
		"pexels": "^1.4.0",
		"roboto-fontface": "*",
		"vue": "^3.4.31",
		"vuetify": "3.7.1"
	},
	"devDependencies": {
		"@babel/types": "^7.24.7",
		"@fortawesome/fontawesome-free": "^6.6.0",
		"@types/node": "^20.14.10",
		"@vitejs/plugin-vue": "^5.0.5",
		"@vue/eslint-config-typescript": "^13.0.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-standard": "^17.1.0",
		"eslint-config-vuetify": "^1.0.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-n": "^16.6.2",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^6.4.0",
		"eslint-plugin-vue": "^9.27.0",
		"i18next-resources-for-ts": "^1.5.0",
		"pinia": "^2.1.7",
		"postcss-nested": "^6.2.0",
		"prettier": "^3.3.3",
		"sass": "1.77.6",
		"typescript": "^5.4.2",
		"unplugin-auto-import": "^0.17.6",
		"unplugin-fonts": "^1.1.1",
		"unplugin-vue-components": "^0.27.2",
		"unplugin-vue-router": "^0.10.0",
		"vite": "^5.3.3",
		"vite-plugin-vue-devtools": "^7.4.6",
		"vite-plugin-vue-layouts": "^0.11.0",
		"vite-plugin-vue-setup-extend": "^0.4.0",
		"vite-plugin-vuetify": "^2.0.3",
